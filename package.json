{
    "name": "web-vue-parcel-bundler",
    "version": "0.0.1",
    "description": "vuejs web type type browser module support with parcel build build and vendor",
    "main": "main.js",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/parcel-bundler/parcel.git"
    },
    "scripts": {
        "build": "parcel build ./main.js -o ./dist/bundle.js",
        "build:bundle": "parcel build ./main.js -o ./dist/bundle.js",
        "build:vendor": "parcel build ./vendor.js",
        "serv": "live-server",
        "dbserv": "json-server --watch ./json-server/db.json"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "collect.js": "^4.0.12",
        "jquery": "^3.3.1",
        "js-cookie": "^2.2.0",
        "json-server": "^0.12.1",
        "live-server": "^1.2.0",
        "parcel-bundler": "^1.6.2",
        "postcss-modules": "^1.1.0",
        "tachyons": "4.9.1",
        "vue": "^2.5.16",
        "vue-ls": "^3.0.0",
        "vue-multianalytics": "^1.9.2",
        "vue-progressbar": "^0.7.4",
        "vue-router": "^3.0.1",
        "vue-select": "^2.4.0",
        "vuex": "^3.0.1",
        "vuex-persistedstate": "^2.5.1",
        "vuex-router-sync": "^5.0.0"
    }
}
