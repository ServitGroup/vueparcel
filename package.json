{
    "name": "web-vue-parcel-bundler",
    "version": "0.0.1",
    "description": "vuejs web type type browser module support with parcel build build and vendor",
    "main": "app.js",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/parcel-bundler/parcel.git"
    },
    "scripts": {
        "build": "parcel build ./app.js -o ./dist/bundle.js",
        "build:bundle": "parcel build ./app.js -o ./dist/bundle.js",
        "build:vendor": "parcel build ./vendor.js"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "collect.js": "^4.0.12",
        "jquery": "^3.3.1",
        "parcel": "^0.0.1",
        "tachyons": "4.9.1",
        "vue": "^2.5.16",
        "vue-multianalytics": "^1.9.2",
        "vue-router": "^3.0.1",
        "vuex": "^3.0.1"
    }
}